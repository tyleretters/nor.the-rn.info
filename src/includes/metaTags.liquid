<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="description" content="{{ site.META.DESCRIPTION }}">
<link rel="canonical" href="{{ site.META.DOMAIN }}{{ page.url }}">
<meta property="og:title" content="{% if title %}{{ title }} | {{ site.META.TITLE }}{% else %}{{ site.META.TITLE }}{% endif %}">
<meta property="og:site_name" content="{{ site.META.TITLE }}">
<meta property="og:url" content="{{ site.META.DOMAIN }}{{ page.url }}">
<meta property="og:type" content="website">
<meta property="og:image" content="{%- render ogImage, site: site, page: page, post: post, release: release, project: project, content: content -%}">
<meta property="og:description" content="{{ site.META.DESCRIPTION }}">
<meta property="og:locale" content="en_US">
<link rel="icon" href="{{ site.META.DOMAIN }}/{{ site.META.FAVICON }}">
<link rel="apple-touch-icon" href="{{ site.META.DOMAIN }}/{{ site.META.APPLE_TOUCH_ICON }}">
